{% if data %}
  <div class="open-weather-block">
    <span class="open-weather-city">{{ data.city }}</span>
    {% for item in data.list %}
      <div>
        <img src="{{ item.icon_url }}">
        <span>{{ item.time }}</span>
        <span>{{ item.temp }}</span>
      </div>
    {% endfor %}
